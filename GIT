git push -u origin master



cd existing_repo
git remote rename origin old-origin
git remote add origin git@gitlab.com:magdyismail/waqf_project.git
git push -u origin --all
git push -u origin --tags




git clone git@gitlab.com:magdyismail/waqf_project.git
cd waqf_project
touch README.md
git add README.md
git commit -m "add README"
git push -u origin master





